{
  "git.mergeEditor": false,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.words": [
    "nuxt"
  ],
  "eslint.workingDirectories": [
    "./packages/*/",
    "www"
  ],
  "eslint.validate": [
    "javascript",
    "typescript",
    "vue"
  ],
}